{
  "name": "encryptor-web-app",
  "version": "1.0.0",
  "description": "The web app's main directory, used for installing and running",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "install-server": "npm install --prefix encryptor-server",
    "install-client": "npm install --prefix encryptor",
    "install-theme": "npx tailwindcss build -o .\"src\"index.css --prefix encryptor",
    "install-all": "npm run install-server && npm run install-client && npm run install-theme && npm install",
    "server": "npm start --prefix encryptor-server",
    "client": "npm start --prefix encryptor",
    "dev": "concurrently \"npm run client \" \"npm run server\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VictorMegir/Encryptor-web-app.git"
  },
  "author": "Victor Megir",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/VictorMegir/Encryptor-web-app/issues"
  },
  "homepage": "https://github.com/VictorMegir/Encryptor-web-app#readme",
  "dependencies": {
    "autoprefixer": "^10.2.5",
    "concurrently": "^6.0.2",
    "postcss": "^8.2.14",
    "tailwindcss": "^2.1.2"
  }
}
